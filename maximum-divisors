answer = 0
count = 0 
tmp = 0

for i in range(1,21):
  number = int(input('enter your number: '))

for k in range(1,number+1):
  if number%k == 0:
    tmp = tmp+1
  else:
    k = k+1

if tmp > count:
  count = tmp
  answer = number
elif tmp == count:
  if number > answer:
    answer = number
    count = tmp
    tmp = 0 

print(answer , count)         
